<div class="container-fluid">
<div class="row row-cols-4 d-flex justify-content-evenly">
{{!-- <div class="card-deck col-sm-3"> --}}
  {{#each posts as |post| }}
    <div class="card">
      <div class="card-body">
        <h5 class="card-title">{{{get_emoji}}} <a href="/post/{{post.id}}">{{post.name}}</a></h5>
        <p class="card-text">{{post.description}}</p>
      </div>
    </div>
  {{/each}}
</div>
</div>
</div>
